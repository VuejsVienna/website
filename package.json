{
  "name": "vuejs-vienna",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "node server.js",
    "now-build": "vue-cli-service build",
    "s3": "npm run build; s3-deploy './dist/**' --cwd './dist/' --signatureVersion v4 --bucket www.vuejs-vienna.org",
    "now": "npm run build && now --public && now alias",
    "ibm": "npm run build; cf push"
  },
  "dependencies": {
    "bootstrap-vue": "^2.0.0-rc.11",
    "express": "^4.16.4",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "node-sass": "^4.9.0",
    "s3-deploy": "^1.1.1",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.17"
  }
}
